{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "node": true,
  },
  "rules": {
    "arrow-parens": [2, "as-needed"],
    "max-len": 0,
    "no-console": 0,
    "no-eval": 0,
    "no-undefined": 1,
    "no-void": 0,
    "prettier/prettier": 2,
    "quotes": [2, "double", "avoid-escape"],
  }
}
